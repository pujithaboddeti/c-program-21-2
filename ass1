#include <stdio.h>
int main()
{
    int s=0,n,i,t;
    printf("enter a number:");
    scanf("%d",&n);
    t=n;
    while(n>0)
    {
        i=n%10;
        s=(s*10)+i;
        n=n/10;
    }
    if(s==t)
        printf("\n%d is a palendrom number",s);
    else
        printf("\n%d not a palendrom number",s);
}
